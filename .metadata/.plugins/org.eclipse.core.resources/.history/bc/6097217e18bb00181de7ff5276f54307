import java.util.Scanner;

public class No_4 {

	public static void main(String[] args) {
		@SuppressWarnings("resource")
		Scanner in =new Scanner(System.in);
        int a=in.nextInt();
        int total=a*100;
        int num=0;
        for(int i=0;i<=total/100;i++) {
        	for(int j=0;j<=(total-100*i)/50;j++) {
        		for(int k=0;k<=(total-1000*i-500*j)/20;k++) {
        			for(int q=0;q<=(total-1000*i-500*j)/10;q++) {
        				for(int w=0;w<=(total-1000*i-500*j-100*q)/5;w++) {
        					for(int e=0;e<=(total-1000*i-500*j-100*q-50*w)/2;e++) {
        						for(int r=0;r<=(total-1000*i-500*j-100*q-50*w-e*20)/1;k++) {
        							for(int k=0;k<=(total-1000*i-500*j-100*q-50*w-e*20)/0.5;k++) {
        								for(int k=0;k<=(total-1000*i-500*j)/0.2;k++) {
        									for(int k=0;k<=(total-1000*i-500*j)/0.1;k++) {
        										for(int k=0;k<=(total-1000*i-500*j)/0.05;k++) {
        											if() {
        												num++;
        											}
        										}
        									}
        								}
        							}
        						}
        					}
        				}
        			}
        		}
        	}
        }
	}
	System.out.println()
}
